{
  "manifest_version": 2,
  "name": "Archiveit",
  "version": "1.1.3",
  "description": "Archive important pieces of information automatically.",
  "background": {
    "page": "background.html"
  },
  "applications": {
    "gecko": {
      "id": "archiveit@minioin.protonmail.com"
    }
  },
  "permissions": [
    "https://web.archive.org/*",
    "bookmarks",
    "history",
    "storage",
    "tabs",
    "unlimitedStorage"
  ]
}
